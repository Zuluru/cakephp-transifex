{
	"name": "dereuromark/cakephp-transifex",
	"type": "cakephp-plugin",
	"description": "The Transifex plugin allows you to pull translations from Transifex Translation API.",
	"homepage": "http://github.com/dereuromark/cakephp-transifex",
	"license": "MIT",
	"authors": [
		{
			"name": "Mark Scherer",
			"homepage": "http://www.dereuromark.de",
			"role": "Maintainer"
		}
	],
	"require": {
		"php": ">=5.4",
		"cakephp/cakephp": "~3.0"
	},
	"support": {
		"source": "https://github.com/dereuromark/cakephp-transifex"
	},
	"autoload": {
		"psr-4": {
			"Transifex\\": "src",
			"Transifex\\Test\\Fixture\\": "tests\\Fixture"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Transifex\\Test\\": "tests",
			"Cake\\Test\\": "vendor/cakephp/cakephp/tests",
			"TestApp\\": "tests/TestApp"
		}
	},
	"extra": {
		"installer-name": "Transifex"
	}
}
